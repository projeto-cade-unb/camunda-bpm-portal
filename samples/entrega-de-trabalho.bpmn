      <bpmn:outgoing>Flow_06ztch1</bpmn:outgoing>
    </bpmn:manualTask>
    <bpmn:sequenceFlow id="Flow_068vdxb" sourceRef="Event_1v8uqvt" targetRef="Activity_13zi7zv" />
