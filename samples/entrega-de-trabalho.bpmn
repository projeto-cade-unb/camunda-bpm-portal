  <bpmn:process id="entrega-de-trabalho" documentation:extendedDocumentation="&#60;p&#62;&#60;strong&#62;Este processo permite a gestão dos trabalhos escolares da instuição de ensino fictícia.&#60;/strong&#62;&#60;/p&#62;&#60;p&#62;O processos consiste em os alunos iniciarem um fluxo e incluirem seus trabalhos escolares e enviar para os professores analisarem, os professores posteriormente vão incluir uma nota e com base nisto o aluno vai receber uma notificação via tarefa com a situação dele que pode ser reprovada, aprovada ou em recuperação.&#60;/p&#62;&#60;p&#62;&#60;/p&#62;&#60;p&#62;&#60;/p&#62;" name="Entrega de Trabalho Escolar" isExecutable="true" camunda:historyTimeToLive="P36500D">
    <bpmn:startEvent id="StartEvent_1" documentation:extendedDocumentation="&#60;p&#62;Aluno Inicia fluxo via formulário inicial sem nenhuma informação.&#60;/p&#62;" name="Entrega de Trabalho Escolar" camunda:initiator="initiator">
      <bpmn:outgoing>Flow_1vr9qxo</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1vr9qxo" sourceRef="StartEvent_1" targetRef="Activity_1ma3bpu" />
    <bpmn:sequenceFlow id="Flow_0cqtwvj" sourceRef="Activity_13zi7zv" targetRef="Activity_1a60v00" />
    <bpmn:userTask id="Activity_1a60v00" name="Professor Recebe Trabalho" camunda:formRef="Form_Professor" camunda:formRefBinding="latest" camunda:candidateGroups="professores" camunda:dueDate="${dateTime().plusDays(1).toDate()}">
      <bpmn:documentation>O professor deve baixar o arquivo do sistema de EAD e aplicar a nota.</bpmn:documentation>
      <bpmn:incoming>Flow_0cqtwvj</bpmn:incoming>
      <bpmn:outgoing>Flow_1vh5lcz</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="Event_0flwzkn" name="Trabalho Recebido">
      <bpmn:incoming>Flow_1i831ep</bpmn:incoming>
      <bpmn:incoming>Flow_13m40z4</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="Activity_13zi7zv" name="Aluno envia Trabalho" camunda:formRef="Form_Aluno" camunda:formRefBinding="latest" camunda:assignee="${initiator}" camunda:dueDate="${dateTime().plusDays(2).toDate()}" camunda:followUpDate="${dateTime().plusDays(1).toDate()}">
      <bpmn:documentation>O Aluno deve enviar o trabalho em arquivo PDF.</bpmn:documentation>
