      <bpmn:incoming>Flow_068vdxb</bpmn:incoming>
      <bpmn:outgoing>Flow_0cqtwvj</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_19qcb6j">
