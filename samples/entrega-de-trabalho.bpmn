    <bpmn:sequenceFlow id="Flow_1vh5lcz" sourceRef="Activity_1a60v00" targetRef="Gateway_19qcb6j" />
    <bpmn:sequenceFlow id="Flow_18g2tu1" name="Nota &#62;= 7" sourceRef="Gateway_19qcb6j" targetRef="Activity_1ibxlzn">
